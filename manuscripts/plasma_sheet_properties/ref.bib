issn = {0004-637X},
journal = {The Astrophysical Journal},
keywords = {Interplanetary turbulence,Plasma astrophysics,Plasma physics,Solar wind},
mendeley-groups = {To be resorted/Daytona MMS Workshop/Dayside,To be resorted/Turbulent reconnection,Projects/Magnetotail PS properties from MMS,Physics topics/Pressure-strain interaction},
month = {apr},
