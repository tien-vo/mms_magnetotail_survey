pages = {1019--1032},
title = {{Quantitative magnetotail characteristics of different magnetospheric states}},
url = {https://angeo.copernicus.org/articles/22/1019/2004/},
volume = {22},
year = {2004}
}
@article{Shustov2019,
abstract = {Magnetic energy release during magnetic reconnection in the magnetotail leads to fast plasma flows transporting thermal energy toward the inner magnetosphere or deep tail. The interaction of such flows with the ambient plasmas is controlled by forces at the flow's leading edge, manifested as a sharp enhancement of the south-north component of magnetic field there, which has been called the dipolarization front. In this study, we examine the kinetic plasma structure of equatorial magnetic field perturbations observed behind dipolarization fronts. Using statistical observations of dipolarization fronts in the near-Earth magnetotail by Time History of Events and Macroscale Interactions during Substorms mission, we find sub-ion scale (scale is below ion gyroradius) magnetic field depressions (magnetic holes), mostly around the equatorial plane, drifting dawnward. They are populated by hot, transversely anisotropic electrons, likely heated around the front. Combining spacecraft observations, analytical estimates, and particle-in-cell simulations, we suggest that these holes result from the ballooning/interchange instability at the dipolarization front. They may represent the nonlinear stage of magnetic field perturbations associated with front instability, which trap hot electrons behind the front. We also discuss the possible role of these holes in scattering and heating electrons and ions in the dipolarized magnetotail.},
author = {Shustov, P. I. and Zhang, X.‐J. and Pritchett, P. L. and Artemyev, Anton and Angelopoulos, Vassilis and Yushkov, E. V. and Petrukovich, A. A.},
doi = {10.1029/2018JA025852},
file = {:C\:/Users/tienr/OneDrive - UCB-O365/Mendeley/Shustov et al. - 2019 - Statistical Properties of Sub‐Ion Magnetic Holes in the Dipolarized Magnetotail Formation, Structure, and Dynami.pdf:pdf},
issn = {2169-9380},
journal = {Journal of Geophysical Research: Space Physics},
keywords = {electron heating,magnetic holes,magnetotail dipolarization},
mendeley-groups = {Projects/Magnetotail PS properties from MMS},
month = {jan},
number = {1},
pages = {342--359},
title = {{Statistical Properties of Sub‐Ion Magnetic Holes in the Dipolarized Magnetotail: Formation, Structure, and Dynamics}},
url = {https://onlinelibrary.wiley.com/doi/10.1029/2018JA025852},
volume = {124},
year = {2019}
}
@article{Sitnov2010,
abstract = {The sufficient stability criterion of the collisionless ion tearing mode in the magnetotail current sheet, which was first obtained by Lembege and Pellat in 1982, is considered. For many conventional 2D current sheet equilibria, this criterion is satisfied within the WKB approximation, which is commonly interpreted as stability of those equilibria with respect to tearing. However, this is not necessarily the case for equilibria with more than two characteristic spatial scales. An example for substantial tearing destabilization of an equilibrium with accumulation of the magnetic flux at the tailward end of a thin current sheet is presented. Similar equilibria are reported in Geotail and THEMIS observations prior to onsets of magnetospheric substorms and dipolarization fronts associated with bursty bulk flows. Copyright 2010 by the American Geophysical Union.},
