journal = {Journal of Geophysical Research: Space Physics},
mendeley-groups = {Projects/Magnetotail PS properties from MMS/Tail ID},
month = {mar},
number = {A3},
pages = {n/a--n/a},
title = {{Statistical properties of tail plasma sheet electrons above 40 keV}},
url = {http://doi.wiley.com/10.1029/2007JA012502},
volume = {113},
year = {2008}
}
@article{Baumjohann1989,
abstract = {Using four months of tail data obtained by the three-dimensional plasma instrument on board the AMPTE/IRM satellite in 1986, we have done a statistical survey on the behavior of ion and electron moments in the central plasma sheet. Almost 80,000 spin averages of plasma density, ion bulk velocity, ion and electron temperature, and plasma $\beta$ were analyzed with respect to differences between their values in the inner and outer central plasma sheet as well as their dependence on magnetic activity. The ion temperature increases with increasing magnetic activity while the ion density decreases during disturbed intervals, except in the neutral sheet neighborhood at smaller radial distances. The ion and electron temperatures in the central plasma sheet are highly correlated, with Ti /Te being constant over a wide range of temperatures and about twice as large as in the distant tail. The average ion flow speeds in the central plasma sheet are below 100 km/s and nearly identical to those found in the plasma sheet boundary layer, although the distribution functions usually are quite different. High-speed flows do occur, but in bursts of most often less than 1 min duration with intermittent intervals of nearly stagnant plasma. The distribution of flow directions strongly favors sunward flow for velocities above 300 km/s, indicating that a near-earth neutral line is rarely, if ever, located inside of XGSM = −19 RE .},
annote = {Abstract:
1. Usage of plasma distribution function to study central sheet statistics of ion and electron moments. Complements Baumjohann+88.
2. Differences between plasma moments in the inner vs. outer sheet & dependence on magnetic activity.
3. Used the absence of spacecraft charging effects in the central plasma sheet to distinguish CPS from PSBL.

Findings:
1. Ion temperature increases with increasing magnetic activity, while ion density decreases during disturbed intervals, except at small radial distances.
2. Ion and electron temperatures are highly correlated.
3. Average ion flow speeds below 100 km/s, identical to those found in the PSBL. But distribution functions are different.
4. High speed flows occur in intermittent bursts less than 1min},
author = {Baumjohann, Wolfgang and Paschmann, Gotz and Cattell, Cynthia A.},
doi = {10.1029/JA094iA06p06597},
file = {:C\:/Users/tienr/OneDrive - UCB-O365/Mendeley/Baumjohann, Paschmann, Cattell - 1989 - Average plasma properties in the central plasma sheet.pdf:pdf},
issn = {01480227},
journal = {Journal of Geophysical Research: Space Physics},
mendeley-groups = {Physics topics/Magnetosphere/Plasma sheet/Central plasma sheet vs. PSBL/Statistical properties of plasma moments,Projects/Magnetotail PS properties from MMS/Previous observations},
month = {jun},
number = {A6},
pages = {6597--6606},
title = {{Average plasma properties in the central plasma sheet}},
url = {http://doi.wiley.com/10.1029/JA094iA06p06597},
volume = {94},
year = {1989}
}
@article{Baumjohann1988,
abstract = {A statistical survey was performed on ion moments (including data on bulk velocity, ion beta, the ratio between the ion pressure and magnetic pressure, and ion temperature anisotropy) near the plasma sheet boundary layer and the immediately adjacent outer central plasma sheet, using four months of tail data obtained by the three-dimensional plasma instrument aboard the AMPTE/IRM satellite. It was found that there is a sharp gradient in ion density and ion beta at the interface between the plasma sheet boundary layer and the central plasma sheet. Average flow velocities in the plasma sheet boundary layer were found to be surprisingly low, typically below 100 km/sec.},
annote = {Paper 1 of Baumjohann+89

Magnetotail criteria: |X_gsm| > 9 Re & |Y_gsm| <= 15 Re

Distinguishing tail regions:
1. Tail lobe: filled with dilute cold plasma, no detection of keV electron/ions. Used partial densities of 1.8-30keV electrons and 8.5-40 keV ions.
2. CPS & PSBL distinctions are a bit trickier. They rejected: high field-aligned flow speed: ion temperature, density, beta; B magnitude & model neutral sheet. Instead, the best criteria is (also see Pedersen+85) to look for photoelectrons.

Findings:
1. The PSBL is a separate region between central sheet vs. lobe with constant values of the moments distinct from the other regions.
2. Sharp gradient in ion density and ion beta at the PSBL-CPS interface.
3. Ion flow velocity in PSBL is typically low (< 100km/s), field-aligned, and directed Earthward.},
author = {Baumjohann, Wolfgang and Paschmann, Gotz and Sckopke, N. and Cattell, Cynthia A. and Carlson, C. W.},
doi = {10.1029/JA093iA10p11507},
file = {:C\:/Users/tienr/OneDrive - UCB-O365/Mendeley/Baumjohann et al. - 1988 - Average ion moments in the plasma sheet boundary layer.pdf:pdf},
issn = {0148-0227},
journal = {Journal of Geophysical Research},
mendeley-groups = {Physics topics/Magnetosphere/Plasma sheet/Central plasma sheet vs. PSBL/Statistical properties of plasma moments,Projects/Magnetotail PS properties from MMS/Previous observations,Projects/Magnetotail PS properties from MMS/Tail ID},
number = {A10},
pages = {11507},
title = {{Average ion moments in the plasma sheet boundary layer}},
url = {http://doi.wiley.com/10.1029/JA093iA10p11507},
volume = {93},
year = {1988}
}
@article{Blake2016,
abstract = {The Energetic Particle Detector (EPD) Investigation is one of five particles and fields investigations on the Magnetospheric Multiscale (MMS) mission. This mission consists of four satellites operating in close proximity in elliptical, low-inclination orbits, and is focused upon the fundamental physics of magnetic reconnection. The Energetic Particle Detector (EPD) investigation aboard the four MMS spacecraft consists of two instrument designs, the EIS (Energetic Ion Spectrometer) and the FEEPS (Fly's Eye Electron Proton Spectrometer). This present paper describes FEEPS from an instrument physics and engineering point of view, and provides some test and calibration data to facilitate effective analysis and use of the flight data for scientific purposes. A FEEPS consists of six Heads, each composed of two Eyes. Each eye is a particle telescope with a single silicon detector; there are nine electron eyes and three ion eyes per FEEPS. The energy coverage is from 25 keV to 650 keV for electrons and 45 keV to 650 keV for ions. Each eye has sixteen energy channels, the spacing of which can be modified by command. The fields of view and pointing of each eye are designed to provide a broad, instantaneous field of view for the twelve eyes per FEEPS. There are two FEEPS per MMS spacecraft mounted such that the pair along with the single EIS provide more than (Formula presented.) -sr instantaneous solid-angle coverage and complete coverage in the equatorial region. A twenty-second spacecraft rotation period is divided into sixty-four sectors to provide detailed temporal and spatial sampling. Data are acquired in three modes: Burst Mode, the primary science mode; Fast Survey Mode; and Slow Survey Mode.},
