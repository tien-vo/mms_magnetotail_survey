journal = {Annales Geophysicae},
keywords = {Magnetospheric physics (Magnetotail,Plasma sheet,Plasma waves and instabilities)},
mendeley-groups = {Projects/Magnetotail PS properties from MMS/Background information},
month = {jan},
number = {1},
pages = {319--328},
title = {{Global properties of magnetotail current sheet flapping: THEMIS perspectives}},
